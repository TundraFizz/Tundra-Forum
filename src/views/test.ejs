<div class="board-index" onselectstart="return false" onmousedown="return false">
  <% for(var i = 0; i < rows.length; i++){ %>
  <div class="board">
    <div class="icon">
      <div style="background-image: url(<%= rows[i]['icon'] %>)"></div>
    </div>
    <div class="info">
      <div class="name">
        <%= rows[i]["name"] %>
      </div>
      <div class="description">
        <%= rows[i]["description"] %>
      </div>
    </div>
    <div class="stats">
      <div class="thread-count">
        <%= rows[i]["thread_count"] %>
      </div>
      <div class="post-count">
        <%= rows[i]["post_count"] %>
      </div>
    </div>
    <div class="last-poster">
    </div>
  </div>
  <% } %>
</div>
